{
  "build": {
    "staging": {
      "android": {
        "releaseChannel": "staging",
        "buildType": "apk"
      },
      "ios": {
        "releaseChannel": "staging"
      },
      "env": {
        "ENTRY_FILE": "./__generated__/AppEntry.js"
      }
    },
    "production": {
      "android": {
        "releaseChannel": "production",
        "buildType": "apk"
      },
      "ios": {
        "releaseChannel": "production"
      },
      "env": {
        "ENTRY_FILE": "./__generated__/AppEntry.js"
      }
    }
  }
}
